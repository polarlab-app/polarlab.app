{
    "topBar": {
        "overview": "General information about your server",
        "settings": "The general settings for your server",
        "logging": "Logs to review your servers' activity",
        "exp-and-leveling": "The best engagement system on Discord"
    },
    "inputs": {
        "channel-logs-status": {
            "name": "Channel Logs",
            "description": "Whether to enable channel logs for your server",
            "path": "config/logs/channelLogs/status",
            "placeholder": null
        },
        "channel-logs-channel": {
            "name": "Channel Logs Channel",
            "description": "The channel for channel logs",
            "path": "config/logs/channelLogs/channelID",
            "placeholder": "Select Channel ..."
        },
        "channel-logs-filter-mode": {
            "name": "Channel Logs Filter Mode",
            "description": "The mode for filtering channels",
            "path": "config/logs/channelLogs/filterMode",
            "options": ["whitelist", "blacklist"]
        },
        "channel-logs-filter": {
            "name": "Filtered Channels",
            "description": "The channels to filter out",
            "path": "config/logs/channelLogs/filter",
            "placeholder": "Select Channels ..."
        },
        "role-logs-status": {
            "name": "Role Logs Status",
            "description": "Whether to enable role logs for your server",
            "path": "config/logs/roleLogs/status",
            "placeholder": null
        },
        "role-logs-channel": {
            "name": "Role Logs Channel",
            "description": "The channel for role logs",
            "path": "config/logs/roleLogs/channelID",
            "placeholder": "Select Channel ..."
        },
        "role-logs-filter-mode": {
            "name": "Role Logs Filter Mode",
            "description": "The mode for filtering roles",
            "path": "config/logs/roleLogs/filterMode",
            "options": ["whitelist", "blacklist"]
        },
        "role-logs-filter": {
            "name": "Filtered Roles",
            "description": "The roles to filter out",
            "path": "config/logs/roleLogs/filter",
            "placeholder": "Select Roles ..."
        },
        "message-logs-status": {
            "name": "Message Logs",
            "description": "Whether to enable message logs for your server",
            "path": "config/logs/messageLogs/status",
            "placeholder": null
        },
        "message-logs-channel": {
            "name": "Message Logs Channel",
            "description": "The channel for message logs",
            "path": "config/logs/messageLogs/channelID",
            "placeholder": "Select Channel ..."
        },
        "message-logs-filter-mode": {
            "name": "Message Logs Filter Mode",
            "description": "The mode for filtering messages",
            "path": "config/logs/messageLogs/filterMode",
            "options": ["whitelist", "blacklist"]
        },
        "message-logs-filter": {
            "name": "Filtered Messages",
            "description": "The messages to filter out",
            "path": "config/logs/messageLogs/filter",
            "placeholder": "Select Messages ..."
        },
        "member-logs-status": {
            "name": "Member Logs",
            "description": "Whether to enable member logs for your server",
            "path": "config/logs/memberLogs/status",
            "placeholder": null
        },
        "member-logs-channel": {
            "name": "Member Logs Channel",
            "description": "The channel for member logs",
            "path": "config/logs/memberLogs/channelID",
            "placeholder": "Select Channel ..."
        },
        "member-logs-filter-mode": {
            "name": "Member Logs Filter Mode",
            "description": "The mode for filtering members",
            "path": "config/logs/memberLogs/filterMode",
            "options": ["whitelist", "blacklist"]
        },
        "member-logs-filter": {
            "name": "Filtered Members",
            "description": "The members to filter out",
            "path": "config/logs/memberLogs/filter",
            "placeholder": "Select Members ..."
        },
        "emoji-logs-status": {
            "name": "Emoji Logs",
            "description": "Whether to enable emoji logs for your server",
            "path": "config/logs/emojiLogs/status",
            "placeholder": null
        },
        "emoji-logs-channel": {
            "name": "Emoji Logs Channel",
            "description": "The channel for emoji logs",
            "path": "config/logs/emojiLogs/channelID",
            "placeholder": "Select Channel ..."
        },
        "emoji-logs-filter-mode": {
            "name": "Emoji Logs Filter Mode",
            "description": "The mode for filtering emojis",
            "path": "config/logs/emojiLogs/filterMode",
            "options": ["whitelist", "blacklist"]
        },
        "emoji-logs-filter": {
            "name": "Filtered Emojis",
            "description": "The emojis to filter out",
            "path": "config/logs/emojiLogs/filter",
            "placeholder": "Select Emojis ..."
        },
        "server-logs-status": {
            "name": "Server Logs",
            "description": "Whether to enable server logs for your server",
            "path": "config/logs/serverLogs/status",
            "placeholder": null
        },
        "server-logs-channel": {
            "name": "Server Logs Channel",
            "description": "The channel for server logs",
            "path": "config/logs/serverLogs/channelID",
            "placeholder": "Select Channel ..."
        },
        "leveling-status": {
            "name": "Leveling",
            "description": "Whether to enable Leveling for your server",
            "path": "config/leveling/status",
            "placeholder": null
        },

        "leveling-exp-amount": {
            "name": "EXP Amount",
            "description": "The amount of EXP to give",
            "path": "config/leveling/amount",
            "min": 0,
            "max": 80,
            "step": 5,
            "placeholder": "Minimum EXP",
            "placeholder2": "Maximum EXP"
        },
        "leveling-exp-type": {
            "name": "EXP Type",
            "description": "The type of EXP to give",
            "path": "config/leveling/type",
            "options": ["static", "range"]
        },
        "leveling-channel-boosters": {
            "name": "Channel Boosters",
            "description": "Per channel EXP Boosters",
            "path": "config/leveling/boosters/channelBoosters",
            "min": "0",
            "max": "6",
            "step": "0.5",
            "limit": "10",
            "placeholder": "Select Channel ..."
        },
        "leveling-role-boosters": {
            "name": "Role Boosters",
            "description": "Per role EXP Boosters",
            "path": "config/leveling/boosters/roleBoosters",
            "min": "0",
            "max": "6",
            "step": ".5",
            "limit": "10",
            "placeholder": "Select Role ..."
        },
        "leveling-member-boosters": {
            "name": "Member Boosters",
            "description": "Per member EXP Boosters",
            "path": "config/leveling/boosters/memberBoosters",
            "min": "0",
            "max": "6",
            "step": ".5",
            "limit": "10",
            "placeholder": "Member ID"
        },
        "leveling-rewards": {
            "name": "Leveling Reward Roles",
            "description": "Per level role rewards",
            "path": "config/leveling/rewards/rewards",
            "limit": "25",
            "placeholder2": "Select Role ...",
            "placeholder": "Level"
        },
        "leveling-reward-status": {
            "name": "Leveling Reward Status",
            "description": "Whether to enable level rewards for your server",
            "path": "config/leveling/rewards/status"
        },
        "leveling-display-status": {
            "name": "Leveling Displaying",
            "description": "Whether to enable level up displaying for your server",
            "path": "config/leveling/display/status"
        },
        "leveling-display-type": {
            "name": "Leveling Channel",
            "description": "The channel for displaying level ups",
            "path": "config/leveling/display/type",
            "options": ["dedicated", "current"]
        },
        "leveling-display-channel": {
            "name": "Leveling Reward Channel",
            "description": "The channel  in which to post level-ups",
            "path": "config/leveling/display/channelID",
            "placeholder": "1131195719676596229"
        },
        "leveling-boosters-status": {
            "name": "Leveling Multipliers",
            "description": "Whether to enable level multipliers for your server",
            "path": "config/leveling/boosters/status"
        },
        "guild-administrators": {
            "name": "Guild Administrators",
            "description": "The users who have access to the dashboard",
            "path": "data/staff",
            "placeholder": "User ID",
            "limit": 10,
            "options": ["dashboardAdministrator", "botAdministrator"]
        }
    },
    "paths": {
        "channel-logs-status": "config/logs/channelLogs/status",
        "channel-logs-channel": "config/logs/channelLogs/channelID",
        "channel-logs-filter-mode": "config/logs/channelLogs/filterMode",
