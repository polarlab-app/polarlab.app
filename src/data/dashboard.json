        "member-logs-status": "config/logs/memberLogs/status",
        "member-logs-channel": "config/logs/memberLogs/channelID",
        "member-logs-filter-mode": "config/logs/memberLogs/filterMode",
        "member-logs-filter": "config/logs/memberLogs/filter",
        "emoji-logs-status": "config/logs/emojiLogs/status",
        "emoji-logs-channel": "config/logs/emojiLogs/channelID",
        "emoji-logs-filter-mode": "config/logs/emojiLogs/filterMode",
        "emoji-logs-filter": "config/logs/emojiLogs/filter",
        "server-logs-status": "config/logs/serverLogs/status",
        "server-logs-channel": "config/logs/serverLogs/channelID",
        "leveling-status": "config/leveling/status",
        "leveling-exp-amount": "config/leveling/amount",
        "leveling-exp-type": "config/leveling/type",
        "leveling-boosters-status": "config/leveling/boosters/status",
        "leveling-role-boosters": "config/leveling/boosters/roleBoosters",
        "leveling-channel-boosters": "config/leveling/boosters/channelBoosters",
        "leveling-member-boosters": "config/leveling/boosters/memberBoosters",
        "leveling-rewards": "config/leveling/rewards/rewards",
